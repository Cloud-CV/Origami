{
  "name": "cvfy",
  "version": "1.0.0",
  "description": "Create a demo of your machine learning model with CloudCV",
  "scripts": {
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node server/buildHTML.js",
    "build": "npm-run-all clean-dist test lint:build build:html && babel-node server/build.js",
    "build:alias": "babel-node server/build.js",
    "start": "node server/start.js",
    "open:src": "babel-node server/srcServer.js",
    "lint": "esw webpack.config.* src server",
    "lint:build": "node_modules/.bin/esw webpack.config.* src server -c ./.eslintrc-build",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "eslint src server webpack.config.* --fix",
    "test": "mocha --reporter spec server/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "dev": "babel-node server/startMessage.js && npm-run-all --parallel open:src lint:watch test:watch"
  },
  "author": "Ashish Chaudhary",
  "license": "AGPL-3.0",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "bcrypt-nodejs": "^0.0.3",
    "eslint-plugin-prettier": "^2.0.1",
    "external-ip": "^0.2.4",
    "jquery": "^3.1.1",
    "load-script": "^1.0.0",
    "material-ui": "^0.17.0",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.8.6",
    "portfinder": "^1.0.13",
    "prettier": "^0.22.0",
    "range_check": "^1.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.11.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-share": "^1.12.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "semantic-ui-css": "^2.2.9",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "toastr": "^2.1.2",
    "victory": "^0.18.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "body-parser": "^1.17.1",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.4",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "expose-loader": "^0.7.3",
    "express": "^4.15.2",
    "express-session": "^1.15.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.9",
    "npm-run-all": "^4.0.2",
    "open": "^0.0.5",
    "passport": "^0.3.2",
    "passport-github": "^1.1.0",
    "react-addons-test-utils": "15.4.2",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-mock-store": "^1.2.2",
    "style-loader": "^0.13.2",
    "superagent": "^3.5.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/batra-mlp-lab/CloudCVfy-Frontend"
  }
}
